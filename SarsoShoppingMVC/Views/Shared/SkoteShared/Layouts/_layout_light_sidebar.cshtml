<!DOCTYPE html>
<html lang="en">

<head>

    @Html.Partial("~/Views/Shared/SkoteShared/_title_meta.cshtml")
    @Html.Partial("~/Views/Shared/SkoteShared/_head_css.cshtml")

</head>

<body data-topbar="dark">

    <!-- Begin page -->
    <div id="layout-wrapper">
        @Html.Partial("~/Views/Shared/SkoteShared/_topbar.cshtml")
        @Html.Partial("~/Views/Shared/SkoteShared/_sidebar.cshtml")

        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->

        <div class="main-content">
            @RenderBody()

            @Html.Partial("~/Views/Shared/SkoteShared/_footer.cshtml")
        </div>
        <!-- end main content-->

    </div>
    <!-- END layout-wrapper -->
    @Html.Partial("~/Views/Shared/SkoteShared/_right_sidebar.cshtml")

    @Html.Partial("~/Views/Shared/SkoteShared/_vendor_scripts.cshtml")

    @RenderSection("scripts", required: false)


</body>

</html>